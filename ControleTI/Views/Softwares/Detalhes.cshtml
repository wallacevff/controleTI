@model Software
@{
    ViewData["Title"] = "Detalhes";
}

<h2>@ViewData["Title"]</h2>

<div>
    <h4>Software</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Software
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Nome)
        </dd>
    </dl>
   
    <a asp-action="Editar" asp-route-id="@Model.Id" class="btn btn-success">Editar</a>
    <a asp-action="index" class="btn btn-primary">Voltar</a>

    <br />
    <br />
    <partial name="_ListarDispositivosSoftwaresPartial" model="Model.DispositivosSoftwares" />
</div>